{
  "manifest_version": 2,
  "name": "rdio controls",
  "description": "play/pause rdio from any tab",
  "version": "0.0.1",
  "permissions": [
    "http://www.rdio.com/"
  ],
  "browser_action": {
    "default_icon": "icon-off.png"
  },
  "background": {
    "scripts": [
      "dist/rdio-controls-chrome-ext.min.js"
    ]
  }
}
